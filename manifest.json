{
    "name": "Lichess Resize",
    "description": "Resizes the Lichess boards so that they look nice",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": ["https://lichess.org/"],
    "content_scripts": [
        {
            "matches": ["https://lichess.org/*"],
            "js": ["resize.js"]
        }
    ]
}
